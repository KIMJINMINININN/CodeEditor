{
  "name": "@org/monaco-zip",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "peerDependencies": {
    "@org/monaco-core": "^0.1.0",
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.6.0",
    "rimraf": "^6.0.1",
    "@org/monaco-core": "workspace:*"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean"
  }
}
